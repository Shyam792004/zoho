int n=sc.nextInt();

       int [] arr=new int[n];
       for(int i=0;i<n;i++)
       {
           arr[i]=sc.nextInt();
       }
       
       int expected=n*(n+1)/2;
       int actual=0;
       for(int i=0;i<n;i++)
       {
          actual+=arr[i]; 
       }
       System.out.println("Missing number :"+ (expected-actual));
